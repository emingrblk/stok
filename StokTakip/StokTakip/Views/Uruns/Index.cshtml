        <th>
            @Html.DisplayNameFor(model => model.UrunIsmi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Adet)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DermojetNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UrunIsmi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Adet)
        </td>
